:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x561712110d90 .scope module, "nand_and" "nand_and" 2 1;
 .timescale 0 0;
L_0x56171215b0f0 .functor NAND 1, v0x56171215adf0_0, v0x56171215aed0_0, C4<1>, C4<1>;
L_0x56171215b210 .functor NAND 1, L_0x56171215b0f0, L_0x56171215b0f0, C4<1>, C4<1>;
v0x56171215adf0_0 .var "A", 0 0;
v0x56171215aed0_0 .var "B", 0 0;
v0x56171215af90_0 .net "F", 0 0, L_0x56171215b210;  1 drivers
v0x56171215b030_0 .net "w1", 0 0, L_0x56171215b0f0;  1 drivers
S_0x561712110f20 .scope begin, "$ivl_for_loop0" "$ivl_for_loop0" 2 12, 2 12 0, S_0x561712110d90;
 .timescale 0 0;
v0x561712112ba0_0 .var/i "i", 31 0;
    .scope S_0x561712110d90;
T_0 ;
    %vpi_call 2 9 "$display", "A B | F" {0 0 0};
    %vpi_call 2 10 "$display", "----|--" {0 0 0};
    %fork t_1, S_0x561712110f20;
    %jmp t_0;
    .scope S_0x561712110f20;
t_1 ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x561712112ba0_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x561712112ba0_0;
    %cmpi/s 4, 0, 32;
    %jmp/0xz T_0.1, 5;
    %load/vec4 v0x561712112ba0_0;
    %parti/s 1, 1, 2;
    %store/vec4 v0x56171215adf0_0, 0, 1;
    %load/vec4 v0x561712112ba0_0;
    %parti/s 1, 0, 2;
    %store/vec4 v0x56171215aed0_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 17 "$display", "%b %b | %b", v0x56171215adf0_0, v0x56171215aed0_0, v0x56171215af90_0 {0 0 0};
    %load/vec4 v0x561712112ba0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x561712112ba0_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %end;
    .scope S_0x561712110d90;
t_0 %join;
    %vpi_call 2 20 "$finish" {0 0 0};
    %end;
    .thread T_0;
